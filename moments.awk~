#!/usr/bin/awk -f
# write out average and std dev from column data
BEGIN {nc = 0}
$1 != "#" { nc++; for(i=1;i<=NF;i++) {sum[i] += $i; ssq[i] += ($i)^2}} 
END { 
	print "# ",nc," Numbers: column, average, stdev";
	for (i=1;i<=NF;i++) {
          printf "%d %f %f \n", i, sum[i]/nc, sqrt((ssq[i]-sum[i]^2/nc)/nc)
        }
   }
